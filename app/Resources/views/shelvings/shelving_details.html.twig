{% extends 'layouts/master.layout.html.twig' %}
{% block title %} Shelving Details {% endblock %}

{% block content %}

    <h3 class="bg-success">{{ shelving.name }}</h3>

    {% for shelf in shelfs %}
      <ul>
          <li>{{ shelf.shelfName}}</li>
      </ul>

     {% endfor %}

    <a href="{{ path('shelvings_list') }}"><button type="button" class="btn btn-primary">Back</button></a>

{% endblock %}